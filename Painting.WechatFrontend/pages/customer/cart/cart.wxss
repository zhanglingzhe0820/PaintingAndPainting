/* pages/order/order.wxss */

.amount {
  width: 88px;
  height: 26px;
  border: 1px solid #ccc;
  border-radius: 3px;
  margin: 0 auto;
  text-align: center;
}

.amount button {
  width: 19px;
  line-height: 26px;
  text-align: center;
  float: left;
}

.amount button::after {
  border: none;
}

.amount input {
  width: 30px;
  height: 26px;
  float: left;
  text-align: center;
  font-size: 12px;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
}

@font-face {
  font-family: "iconfont";
  src: url('iconfont.eot?t=1517499448084'); /* IE9*/
  src: url('iconfont.eot?t=1517499448084#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('iconfont.ttf?t=1517499448084') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+*/ url('iconfont.svg?t=1517499448084#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-delete:before {
  content: "\e69d";
}

.table {
  height:30px;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
}
